{
  "name": "Oral-clinic",
  "productName": "Oral-clinic",
  "version": "1.0.0",
  "description": "Aplicación para gestión odontologica",
  "license": "MIT",
  "repository": "suiteapp/oral-clinic",
  "homepage": "https://www.oralclinic.org",
  "author": {
    "name": "oral-clinic",
    "email": "hello@oralclinic.org",
    "url": "https://oralclinic.org"
  },
  "private": true,
  "main": "main/index.js",
  "scripts": {
    "start": "yarn && electron .",
    "build": "next build renderer && next export renderer",
    "dist": "npm run build && build",
    "pack": "npm run build && build --dir",
    "postinstall": "electron-builder install-app-deps",
    "release-win": "electron-packager . OralClinic --overwrite --platform=win32 --arch=ia32 --icon='./build/icon.ico' --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='OralClinic App'"
  },
  "bundle": {
    "name": "OralClinic"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.9",
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/pickers": "^3.2.4",
    "@material-ui/styles": "^4.0.0",
    "@sentry/browser": "^4.4.1",
    "@sentry/electron": "^0.14.0",
    "@sindresorhus/do-not-disturb": "^0.2.0",
    "@sindresorhus/to-milliseconds": "^1.0.0",
    "@zeit/next-css": "^1.0.1",
    "ajv": "^6.6.1",
    "aperture": "^5.1.0",
    "axios": "^0.19.0",
    "base64-img": "^1.0.4",
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "css-loader": "^3.2.0",
    "date-fns": "^2.0.1",
    "delay": "^4.1.0",
    "electron-better-ipc": "^0.1.1",
    "electron-contextmenu-middleware": "^1.0.3",
    "electron-input-menu": "^2.1.0",
    "electron-is-dev": "^1.0.1",
    "electron-log": "^2.2.17",
    "electron-next": "^3.1.5",
    "electron-packager": "^15.0.0",
    "electron-store": "^2.0.0",
    "electron-updater": "^4.0.6",
    "electron-util": "^0.10.2",
    "execa": "^1.0.0",
    "file-icon": "^3.0.0",
    "first-run": "^1.2.0",
    "fs": "0.0.1-security",
    "got": "^9.4.0",
    "hide-desktop-icons": "^0.4.0",
    "husky": "^1.2.0",
    "insight": "^0.10.1",
    "lodash": "^4.17.10",
    "make-dir": "^1.3.0",
    "moment": "^2.29.2",
    "moment-range": "^4.0.2",
    "move-file": "^1.0.0",
    "mui-datatables": "^2.6.3",
    "nearest-normal-aspect-ratio": "^1.2.1",
    "nedb": "^1.8.0",
    "next-images": "^1.1.1",
    "npm": "^6.5.0",
    "p-cancelable": "^1.0.0",
    "p-event": "^2.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-calendar-material": "^0.1.8",
    "react-dom": "^16.6.3",
    "react-linkify": "^0.2.2",
    "request": "^2.88.0",
    "semver": "^5.6.0",
    "style-loader": "^1.0.0",
    "tempy": "^0.2.1",
    "tildify": "^1.2.0",
    "tmp": "^0.0.33",
    "unstated": "^1.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "electron": "5.0.4",
    "electron-builder": "^20.38.3",
    "eslint-config-xo-react": "^0.18.0",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^4.0.0",
    "next": "8.1.0",
    "url-loader": "^2.0.1",
    "xo": "^0.24.0"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": "xo-react",
    "space": 2,
    "envs": [
      "node",
      "browser"
    ],
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "import/no-unassigned-import": "off",
      "no-use-before-define": "off",
      "react/jsx-closing-tag-location": "off",
      "react/require-default-props": "off",
      "react/no-unused-prop-types": "off",
      "react/jsx-curly-brace-presence": "off"
    },
    "ignores": [
      "dist",
      "renderer/.next",
      "renderer/out"
    ]
  },
  "husky": {
    "hooks": {}
  },
  "build": {
    "productName": "OralClinic",
    "target": "NSIS",
    "directories": {
      "output": "./release"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "extraResources": [
      "./extraResources/**"
    ],
    "appId": "com.oralclinic.suiteapp.org",
    "files": [
      "**/*",
      "!renderer",
      "renderer/out"
    ]
  }
}
